<admintpl file="header" />
</head>
<body>
	<div class="wrap js-check-wrap">
		<ul class="nav nav-tabs">
			<li class="active"><a href="{:U('code_list')}">授权详情列表</a></li>
		</ul>
		<form class="well form-search" method="post" action="{:U('dev_auth_list')}">
			<!--类型：
			<select id="type" name="type" style="width: 80px;">
				<option value="-1"<if condition="$type eq -1">selected="selected"</if>>全部</option>
				<option value="0" <if condition="$type eq 0">selected="selected"</if>>陌陌</option>
				<option value="1" <if condition="$type eq 1">selected="selected"</if>>微信</option>
			</select>-->
			uuid:
			<input class="input" type="text" name="unique_id" style="width: 100px;font-size:9px;" value="{$unique_id}" placeholder="请输入uuid">&nbsp;
			设备名称:
			<input class="input" type="text" name="dev_name" style="width: 100px;font-size:9px;" value="{$dev_name}" placeholder="请输入设备名称">&nbsp;
			授权码:
			<input class="input" type="text" name="auth_code" style="width: 100px;font-size:9px;" value="{$auth_code}" placeholder="请输入授权码">&nbsp;
			到期时间：
			<input type="text" name="start_time" class="js-datetime" value="{$start_time|default=''}" style="width: 130px;" placeholder="起始时间" autocomplete="off">-
			<input type="text" class="js-datetime" name="end_time" value="{$end_time|default=''}" style="width: 130px;" placeholder="结束时间" autocomplete="off"> &nbsp; &nbsp;
			<input type="submit" class="btn btn-primary" value="搜索">
			<a class="btn btn-danger" href="javascript:void(0)" onclick="clearall()">清空</a>
			<!--<a class="btn btn-danger" href="javascript:void(0)" onclick="toexcel()">导出excel</a>-->
		</form>
		<table class="table table-hover table-bordered">
			<thead>
				<tr>
					<th width="50">序号</th>
					<th>设备名称</th>
					<th>设备唯一标识</th>
					<th>设备序列号</th>
					<th>设备imei</th>
					<th>授权码</th>
					<th>到期时间</th>
					<th>授权IP</th>
					<!--<th width="120">操作</th>-->
				</tr>
			</thead>
			<tbody>
				<foreach name="list" item="vo">
				<tr>
					<td>{$vo.id}</td>
					<td>{$vo.dev_name}</td>
					<td>{$vo.unique_id}</td>
					<td>{$vo.serial_number}</td>
					<td>{$vo.imei}</td>
					<td>{$vo.auth_code}</td>
					<td>{:date("Y-m-d H:i",$vo['overtime'])}</td>
					<td>{$vo.auth_ip}</td>
				</tr>
				</foreach>
			</tbody>
		</table>
		<div class="pagination">{$page}</div>
	</div>
	<script src="__PUBLIC__/js/common.js"></script>
    <script>
		function clearall(){
			$('.input').val("");
		}
	</script>
</body>
</html>