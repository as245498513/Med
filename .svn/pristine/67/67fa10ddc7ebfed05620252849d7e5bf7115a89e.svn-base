<admintpl file="header" />
</head>
<body>
	<div class="wrap js-check-wrap">
		<ul class="nav nav-tabs">
			<li class="active"><a href="{:U('index')}">陌陌授权码</a></li>
			<li><a href="{:U('add')}">添加授权码</a></li>
		</ul>
        <form class="well form-search" method="post" action="{:U('User/index')}">
           
            <input type="submit" class="btn btn-primary" value="搜索" />
            <a class="btn btn-danger" href="{:U('index')}">清空</a>
        </form>
		<table class="table table-hover table-bordered">
			<thead>
				<tr>
					<th width="50">ID</th>
					<th>注册码</th>
					<th>创建时间</th>
					<th>有效期限（天）</th>
					<th>手机序列号</th>
					<th>手机UDID</th>
					<th>使用时间</th>
					<th width="120">操作</th>
				</tr>
			</thead>
			<tbody>

				<foreach name="list" item="vo">
				<tr>
					<td>{$vo.id}</td>
					<td>{$vo.reg_code}</td>
					<td>{:date("Y-m-d H:i",$vo['createtime'])}</td>
					<td>{$vo['duration']}</td>
					<td>{$vo.serial_number}</td>
					<td>{$vo.udid}</td>
					<td>{:date("Y-m-d H:i",$vo['settime'])}</td>
					<td>
						
					</td>
				</tr>
				</foreach>
			</tbody>
		</table>
		<div class="pagination">{$page}</div>
	</div>
	<script src="__PUBLIC__/js/common.js"></script>
</body>
</html>